# Record template for DT320 temperature controller
#ai record template for register inputs
record(bi, "$(P):TT$(ADR):sOut1") {
    field(DESC,"Output 1")
    field(DTYP,"asynInt32")
    field(INP,"@asyn($(PORT) 4138 -16)MODBUS_DATA")
    field(SCAN,"$(SCAN)")
    field(ZNAM, "Off")
    field(ONAM, "On")
    field(MASK, "0x40")
}
